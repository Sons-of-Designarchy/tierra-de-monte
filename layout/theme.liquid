  <script>
    (function () {
      function paintMobileBorders() {
        var isMobile = window.matchMedia('(max-width: 749.98px)').matches;
        var items = document.querySelectorAll(
          '.multicolumn .mc-feature__cols ul.multicolumn-list > li.multicolumn-list__item,' +
            '.multicolumn slider-component ul.multicolumn-list > li.multicolumn-list__item'
        );

        items.forEach(function (li) {
          if (isMobile) {
            li.style.setProperty('border-left', '1px solid rgb(var(--color-foreground))', 'important');
            li.style.setProperty('padding-left', '8px', 'important');
          } else {
            li.style.removeProperty('border-left');
            li.style.removeProperty('padding-left');
          }
        });
      }

      document.addEventListener('DOMContentLoaded', paintMobileBorders);
      document.addEventListener('shopify:section:load', paintMobileBorders);
      window.addEventListener('resize', paintMobileBorders);
    })();
  </script>

