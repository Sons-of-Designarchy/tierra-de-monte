{
  "sections": {
    "breadcrumb": {
      "type": "custom-breadcrumb-blog",
      "settings": {
        "back_label": "Back",
        "color_scheme": "scheme-1",
        "padding_top": 12,
        "padding_bottom": 12
      }
    },
    "main": {
      "type": "main-article",
      "blocks": {
        "title": {
          "type": "title",
          "settings": {
            "blog_show_date": true,
            "blog_show_author": true
          }
        },
        "share": {
          "type": "share",
          "settings": {
            "share_label": "Share"
          }
        },
        "featured_image": {
          "type": "featured_image",
          "settings": {
            "image_height": "medium"
          }
        },
        "content": {
          "type": "content",
          "settings": {}
        }
      },
      "block_order": [
        "title",
        "share",
        "featured_image",
        "content"
      ],
      "settings": {}
    }
  },
  "order": [
    "breadcrumb",
    "main"
  ]
}
